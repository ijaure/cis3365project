<AnchorPane prefHeight="437.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ReportsTableController">
   <children>
      <HBox prefHeight="60.0" prefWidth="607.0" style="-fx-background-color: #916D91;">
         <children>
            <Label text="REPORTS TABLE" textFill="#f8f3f8">
               <font>
                  <Font name="Microsoft YaHei UI Light" size="29.0" />
               </font>
               <HBox.margin>
                  <Insets left="25.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
    <AnchorPane layoutY="60.0" minHeight="0.0" minWidth="0.0" prefHeight="366.0" prefWidth="607.0">
         <children>
            <HBox layoutY="318.0" prefHeight="60.0" prefWidth="607.0" style="-fx-background-color: #E9E6E9;">
               <children>
                  <HBox prefHeight="55.0" prefWidth="482.0" spacing="3.0">
                     <children>
                        <MenuButton mnemonicParsing="false" prefHeight="36.0" prefWidth="224.0" style="-fx-background-color: #916D91;" text="Please Select a Report" textFill="WHITE">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <font>
                              <Font name="Microsoft JhengHei UI" size="17.0" />
                           </font>
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#queryVendorStatusName" text="Show Vendors by Status Name" />
                              <MenuItem mnemonicParsing="false" onAction="#queryVendorState" text="Show Vendors by State" />
                              <MenuItem mnemonicParsing="false" onAction="#queryProductVendorOrder" text="Show Vendors, Products, &amp; Their Orders" />
                              <MenuItem mnemonicParsing="false" onAction="#queryInactiveVendor" text="Show Inactive Vendors &amp; Their Products" />
                              <MenuItem mnemonicParsing="false" onAction="#queryEmployeeStatusName" text="Show Employees by Status Name" />
                              <MenuItem mnemonicParsing="false" onAction="#queryOrdersHold" text="Show Orders On-Hold" />
                           </items>
                           <HBox.margin>
                              <Insets left="200.0" top="6.0" />
                           </HBox.margin>
                        </MenuButton>
                     </children>
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </HBox>
               </children>
            </HBox>
            <ScrollPane hbarPolicy="ALWAYS" prefHeight="320.0" prefViewportHeight="318.0" prefViewportWidth="982.0" prefWidth="607.0" vbarPolicy="NEVER">
               <content>
                  <TableView fx:id="dynamicTable" prefHeight="318.0" prefWidth="700.0" />
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
